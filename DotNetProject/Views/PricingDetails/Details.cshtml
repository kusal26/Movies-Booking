@model DotNetProject.Models.PricingDetails

@{
    ViewData["Title"] = "Details";
}
@{
    if (ViewBag.IsAdmin = true)
    {
        Layout = "_AdminLayout";
    }
    else
    {
        Layout = "_Layout";
    }
}
<h1>Details</h1>

<div>
    <h4>PricingDetails</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Price)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-outline-succcess" style="text-decoration:none" asp-route-id="@Model?.PriceId">Edit</a> |
    <a asp-action="Index" class="btn btn-outline-danger" style="text-decoration:none">Back</a>
</div>
